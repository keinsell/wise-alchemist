{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "test": {
      "inputs": ["src/**/*.test.ts"],
      "dependsOn": ["^build", "^db:generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "cache": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build", "^db:generate", "^db:push"]
    },
    "db:migrate:deploy": {
      "outputs": [],
      "cache": false
    },
    "db:push": {
      "outputs": [],
      "cache": false
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "db:studio": {
      "outputs": [],
      "cache": false
    },
    "db:generate": {
      "dependsOn": ["^db:generate"],
      "outputs": ["node_modules/@prisma/client/generator-build/index.js"],
      "cache": true
    },
    "hephaistos": {
      "inputs": ["src/**/*.ts"],
      "outputs": ["cache/**/*.json"],
      "dependsOn": ["^build", "^db:generate"],
      "cache": true
    }
  },
  "remoteCache": {
    "signature": true
  }
}
